{
  "manifest_version": 2,
  "name": "Speed Test",
  "description": "Tests speed",
  "version": "1.0.0",
  "icons": {"128": "./images/speed.png"},
  
  "browser_action":{
    "default_icon": "./images/speed.png",
    "default_popup": "popup.html"
  },

  "chrome_url_overrides": {
    "newtab": "popup.html"
  },

  "permissions": ["activeTab", "http://api.jambase.com/*"],
  "content_security_policy": "script-src 'self' https://api.eventful.com/js/api; object-src 'self'"
}